<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="374.0" prefWidth="465.0" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ordenamiento.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <AnchorPane prefHeight="344.0" prefWidth="426.0">
      <children>
      
        <Label fx:id="welcomeText" layoutX="188.0" layoutY="58.0" prefHeight="18.0" prefWidth="47.0" />

          <Button fx:id="timPage" layoutX="159.0" layoutY="76.0" mnemonicParsing="false" onAction="#showTimWindow" prefHeight="26.0" prefWidth="108.0" text="TIM SORT" />
          <Button fx:id="combPage" layoutX="159.0" layoutY="115.0" mnemonicParsing="false" onAction="#showCombWindow" prefHeight="26.0" prefWidth="108.0" text="COMB SORT" />
          <Button fx:id="selectionPage" layoutX="159.0" layoutY="156.0" mnemonicParsing="false" onAction="#showSeleccionWindow" text="SELECTION SORT" />
          <Button fx:id="reportPage" layoutX="326.0" layoutY="293.0" mnemonicParsing="false" onAction="#showReportWindow" prefHeight="26.0" prefWidth="78.0" text="REPORTE" />
          <Button fx:id="treePage" layoutX="159.0" layoutY="197.0" mnemonicParsing="false" onAction="#showTreeWindow" prefHeight="26.0" prefWidth="108.0" text="TREE SORT" />

          <Label layoutX="101.0" layoutY="23.0" prefHeight="20.0" prefWidth="224.0" text="ALGORITMOS DE ORDENAMIENTO">
            <font>
               <Font size="14.0" />
            </font></Label>
      </children>
   </AnchorPane>
</VBox>
